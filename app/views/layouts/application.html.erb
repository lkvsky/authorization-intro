<!DOCTYPE html>
<html>
<head>
  <title>Authentication</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag "custom", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= button_to "Log Out!", session_path, :method => :delete, :id => "log-out", :class => "btn btn-primary" %>
  <p><%= flash.notice %></p>
<%= yield %>

</body>
</html>
